{
	"name": "imba-shell",
	"version": "0.5.0",
	"description": "Interactive debugger and REPL for Imba.",
	"keywords": [
		"imba",
		"imba-shell",
		"shell",
		"repl",
		"debugger"
	],
	"author": "Donald Pakkies",
	"license": "MIT",
	"main": "lib/index.js",
	"types": "types/index.d.ts",
	"bugs": "https://github.com/donaldp/imba-shell/issues",
	"homepage": "https://github.com/donaldp/imba-shell",
	"repository": "https://github.com/donaldp/imba-shell",
	"scripts": {
		"build": "npm run imba:build && npm run imba:build:compilers && npm run imba:build:errors && npm run imba:build:runners",
		"imba:build": "imba build src/index.imba --platform=node --outdir=lib -f -M -S",
		"imba:build:compilers": "imba build src/Compilers/index.imba --platform=node --outdir=lib/Compilers -f -M -S",
		"imba:build:errors": "imba build src/Errors/index.imba --platform=node --outdir=lib/Errors -f -M -S",
		"imba:build:runners": "imba build src/Runners/index.imba --platform=node --outdir=lib/Runners -f -M -S",
		"test": "jest --roots test",
		"test:watch": "jest --watchAll --roots test",
		"prepare": "npm run build && npm run test"
	},
	"bin": {
		"imba-r": "./bin/runtime",
		"imba-runtime": "./bin/runtime",
		"imba-shell": "./bin/repl",
		"imbar": "./bin/runtime",
		"imbas": "./bin/repl",
		"ir": "./bin/runtime"
	},
	"exports": {
		".": "./lib/index.js",
		"./Compilers": "./lib/Compilers/index.js",
		"./Errors": "./lib/Errors/index.js",
		"./Runners": "./lib/Runners/index.js"
	},
	"dependencies": {
		"imba": "^2.0.0-alpha.225",
		"typescript": "^4.8.4"
	},
	"devDependencies": {
		"@types/jest": "^29.2.2",
		"@types/node": "^18.11.9",
		"jest": "^29.3.1"
	}
}
